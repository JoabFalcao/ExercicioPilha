class Pilha:
  def __init__(self):
    self._itens = []
  def isVazio(self):
    if (len(self._itens) == 0):
      return "Vazio"
  def empilhar(self, item):
    self.itens.append(item)
  def desempilhar(self, item):
    self.itens.pop(item)
  def peek(self):
    return self.items[len(self.items)-1]
  def lenght(self):
    return len(self.items)

pilha = Pilha()
